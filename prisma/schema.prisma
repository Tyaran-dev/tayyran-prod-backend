generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model Hotel {
  id           String    @id @default(cuid()) @map("hotel_id")
  hotel_code   String    @unique
  name         String
  address      String?
  city_code    String
  city_name    String?
  country_name String?
  star_rating  String?   
  image_urls   String[] 
  
  @@map("hotels")
}